{
    "name": "rtc-ui",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,mjs}\" --color --max-warnings=0",
        "prettier": "prettier \"**/*.ts\" \"**/*.tsx\" --write",
        "build-server": "docker build --file docker/Dockerfile -t webrtc-node-server .",
        "start-server": "docker run -p 8080:8080 -d webrtc-node-server:latest",
        "test": "jest",
        "prepare": "husky",
        "sync": "sh scripts/sync-test-time.sh"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*": [
            "yarn run sync"
        ]
    },
    "dependencies": {
        "istanbul": "^0.4.5",
        "lucide-react": "^0.514.0",
        "minimist": "^1.2.8",
        "next": "15.3.3",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "recharts": "^2.15.3"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.27.1",
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/jest": "^30.0.0",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "15.3.3",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.1",
        "husky": "^9.1.7",
        "jest": "^30.0.5",
        "jest-environment-jsdom": "^30.0.5",
        "lint-staged": "^16.1.5",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.12",
        "tailwindcss": "^4",
        "ts-jest": "^29.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5"
    }
}
